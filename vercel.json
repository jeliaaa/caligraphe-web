{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "NPM_FLAGS": "--legacy-peer-deps"
    },
    "build": {
        "env": {
            "VERCEL_BUILDER_DEBUG": "1"
        }
    }
}